<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://www.example.org/helloworld/"
                  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="helloworld"
                  targetNamespace="http://www.example.org/helloworld/">
    <wsdl:types>
        <xsd:schema targetNamespace="http://www.example.org/helloworld/">
            <xsd:element name="inputSOATest">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="test" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="outputSOATest">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="result" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:schema>
    </wsdl:types>

    <wsdl:message name="inputSOATest">
        <wsdl:part element="tns:inputSOATest" name="in"/>
    </wsdl:message>
    <wsdl:message name="outputSOATest">
        <wsdl:part element="tns:outputSOATest" name="out"/>
    </wsdl:message>
    <wsdl:portType name="SOATestEndpoint">
        <wsdl:operation name="SOATest">
            <wsdl:input message="tns:inputSOATest"/>
            <wsdl:output message="tns:outputSOATest"/>
        </wsdl:operation>
    </wsdl:portType>
    <wsdl:binding name="SOATestBinding" type="tns:SOATestEndpoint">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsdl:operation name="SOATest">
            <soap:operation soapAction="http://www.example.org/helloworld/NewOperation"/>
            <wsdl:input>
                <soap:body use="literal"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body use="literal"/>
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>
    <wsdl:service name="SOATestEndpointService">
        <wsdl:port binding="tns:SOATestBinding" name="SOATestEndpoint">
            <soap:address location="http://localhost:8080/ws/helloWorldExample"/>
        </wsdl:port>
    </wsdl:service>
</wsdl:definitions>